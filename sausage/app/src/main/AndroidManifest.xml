<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gzhang.sausage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BuyerHomeActivity"></activity>
        <activity android:name=".BuyerItemDescriptionActivity"></activity>
        <activity android:name=".BuyerTermsOfAgreementActivity"></activity>
        <activity android:name=".BuyerListOfItemsActivity"></activity>
        <activity android:name=".BuyerFileDisputeEvidenceActivity"></activity>
        <activity android:name=".BuyerListOfDisputesActivity"></activity>
        <activity android:name=".SellerHomeActivity"></activity>
        <activity android:name=".SellerProvideDisputeEvidenceActivity"></activity>
        <activity android:name=".SellerListOfDisputesActivity"></activity>
        <activity android:name=".VoterDisputesListActivity"></activity>
        <activity android:name=".DisputeBuyerAndSellerActivity"></activity>
        <activity android:name=".Solution"></activity>
        <activity android:name=".Accept"></activity>
    </application>

</manifest>